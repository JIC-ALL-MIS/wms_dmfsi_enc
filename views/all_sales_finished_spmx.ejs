<!DOCTYPE html>
<html lang="en">

<head>
    <%- include('./partials/head'); -%>

</head>

<body>
    <!--*******************
    Preloader start
********************-->
    <div id="preloader">
        <div class="sk-three-bounce">
            <div class="sk-child sk-bounce1"></div>
            <div class="sk-child sk-bounce2"></div>
            <div class="sk-child sk-bounce3"></div>
        </div>
    </div>
    <!--*******************
    Preloader end
********************-->

    <!--**********************************
    Main wrapper start
***********************************-->
    <div id="main-wrapper">

        <!--**********************************
        Nav header start
    ***********************************-->
        <%- include('./partials/header_logo'); -%>

            <!--**********************************
        Nav header end
    ***********************************-->

            <!--**********************************
        Header start
    ***********************************-->
            <%- include('./partials/header',{titel: "Outgoing for Spices / Mix"}); -%>

                <!--**********************************
        Header end ti-comment-alt
    ***********************************-->

                <!--**********************************
        Sidebar start
    ***********************************-->
                <%- include('./partials/sidebar'); -%>

                    <!--**********************************
        Sidebar end
    ***********************************-->

                    <div class="content-body">
                        <div class="container-fluid">
                            <!-- row -->

                            <div class="col-lg-12">
                                <div class="card">
                                    
                                    <div class="card-header">
                                        <h4 class="card-title col-6"><%= language.all_purchase %> </h4>
                                        <div class="card-title col-6 text-end">

                                            <a href="/all_sales_finished/view/add_sales_spmx" class="btn btn-rounded btn-info text-white"> Add Outgoing
                                                <span class="btn-icon-start text-info">
                                                    <i class="fa fa-plus color-info"></i>
                                                </span>
                                            </a>

                                        </div>
                                    </div>

                                    <div class="card-body">

                                        <div class="d-none">
                                            <input type="hidden" value="<%= master_shop[0].currency %>" id="hidden_currency">
                                            <input type="hidden" value="<%= master_shop[0].currency_placement %>" id="hidden_currency_placement">
                                        </div>
                                        
                                        <div class="table-responsive">
                                            <table id="example" class="table table-responsive-sm text-center" width="100%">
                                                <thead>
                                                    <tr>
                                                        <th>DOC NO.</th>
                                                        <th>DATE </th>
                                                        <th>STORAGE </th>
                                                        <th>ROOM </th>
                                                        <th>PRODUCT CODE</th>
                                                        <th>SPICEMIX CODE</th>
                                                        <th>QTY  </th>
                                                        <th>EXTENDERS CODE</th>
                                                        <th>QTY  </th>
                                                        <th>CURING SALT CODE</th>
                                                        <th>QTY  </th>
                                                        <th>ACTION </th>
                                                        <th>STATUS </th>
                                                        
                                                    </tr>
                                                </thead>
                                                <tbody>

                                                    <% data.forEach(element => { %>
                                                        <tr>
                                                            <td><%= element.invoice %></td>
                                                            <td><%= element.date %></td>
                                                            <td><%= element.warehouse_name %></td>
                                                            <td><%= element.room_spmx %></td>
                                                            <td><%= element.product_code %></td>
                                                            <td><%= element.spicemix_spmx %></td>
                                                            <td><%= element.spicemix_spmx_qty %></td>
                                                            <td><%= element.extenders_spmx %></td>
                                                            <td><%= element.extenders_spmx_qty %></td>
                                                            <td><%= element.curing_spmx %></td>
                                                            <td><%= element.curing_spmx_qty %></td>
                                                            <td>
                                                                <a href="/all_sales_finished/view_sales_spmx/<%= element._id %>" id="barcode" data-id="<%= element._id %>"
                                                                    class="btn btn-success btn-sm">
                                                                    <i class="feather icon-printer"> </i>View
                                                                </a>                                                            </td>
                                                            <td>
                                                                <% if (element.finalize == "True") { %>
                                                                    <span class="badge rounded-pill bg-success">
                                                                        Finalize
                                                                    </span>
                                                                <% } else { %>
                                                                    <span class="badge rounded-pill bg-danger">
                                                                        Not Finalize
                                                                    </span>
                                                                <% } %>
                                                                
                                                            </td>
                                                        </tr>
                                                    <% }) %>
                                                </tbody>

                                            </table>
                                        </div>
                                    </div>
                                </div>
                            </div>
                        </div>
                    </div>

                    <!-- Payment modal end -->

                    <!--**********************************
            Footer start
        ***********************************-->
                    <%- include('./partials/footer'); -%>

                        <!--**********************************
            Footer end
        ***********************************-->

    </div>
    <!--**********************************
        Main wrapper end
    ***********************************-->

    <!-- start Toastr -->
    <%- include('./partials/toastr'); -%>
        <!-- End Toastr -->

        <!--**********************************
        Scripts
    ***********************************-->
       

        <%- include('./partials/script'); -%>

            <!--**********************************
        Scripts end
    ***********************************-->
</body>

</html>